   'X-CSRFToken': csrftoken,\r\n            },\r\n            body: JSON.stringify(obj) //JavaScript object of data to POST\r\n        }).then((res) => { return res.blob(); })\r\n        .then((data) => {\r\n          var a = document.createElement(\"a\");\r\n          a.href = window.URL.createObjectURL(data);\r\n          a.download = \"ToDo.txt\";\r\n          a.click();\r\n        });\r\n\r\n        console.log(\"done\");\r\n    }\r\n\r\n    return(\r\n        <div className='ultra-flex'>\r\n            <div className=\"Calctitle\">Note-taking</div>\r\n            <div className='mega-flex'>\r\n            <div className='header'>\r\n                <input ref={input2ref} className='Note-topic'></input>\r\n                <button className='controlb1' onClick={event => {makeNote()}} >Add Note</button>\r\n                <button className='controlb1' onClick={event => {exportNotes()}} >Export to textfile</button>\r\n            </div>\r\n            <div ref={noteref} className='note-list'>\r\n                {children}\r\n            </div>\r\n        </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Notes;","C:\\Users\\Safi\\Desktop\\Safi\\projects\\Django project\\django_widget_panel\\widgets\\widgets\\react_stuff\\frontend\\src\\note_taking\\note.js",[],[],"C:\\Users\\Safi\\Desktop\\Safi\\projects\\Django project\\django_widget_panel\\widgets\\widgets\\react_stuff\\frontend\\src\\currencyConv\\Conv.js",[],[],"C:\\Users\\Safi\\Desktop\\Safi\\projects\\Django project\\django_widget_panel\\widgets\\widgets\\react_stuff\\frontend\\src\\currencyConv\\combobox.js",["50"],[],"import './Conv.css';\r\nimport { useRef } from 'react';\r\n\r\n\r\nfunction Combo(props){\r\n\r\n    let arr = ['AED','AFN','ALL','AMD','ANG','AOA','ARS','AUD','AWG','AZN','BAM','BBD','BDT','BGN','BHD','BIF','BMD','BND','BOB','BRL','BSD','BTN','BWP','BYR','BZD','CAD','CDF','CHF','CLP','CNY','COP','CRC','CUP','CVE','CZK','DJF','DKK','DOP','DZD','EEK','EGP','ERN','ETB','EUR','FJD','FK